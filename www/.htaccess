#JAVASCRIPT ERRORS
RewriteRule ^(.*)/NaN/?$ $1/1
RewriteRule ^(.*)/undefined/?$ $1/1

#MINIFY
RewriteRule ^/_etc/min/([a-z]=.*)  /_etc/min/index.php?$1  [L,NE]

#IMAGES
RewriteCond %{REQUEST_URI} ^.*/_etc/resources/.*$
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^_etc/resources/cut/([^-]*)-([^-]*)-([^-]*)-([^-]*)-([^-]*)-([^-]*)-([^-]*)-([^-]*)\.([^-]*)$ /_etc/cgi\/images\/serve.php?imgfile=$1\.$9&rnd=$2&aspect=$3&platform=$4&columns=$5&border=$6&processing=$7&psettings=$8 [T=application/x-httpd-cgi,PT,L]

#CACHE
RewriteCond %{REQUEST_URI} ^.*_etc/cache/.*$
RewriteCond %{REQUEST_FILENAME} !-s
RewriteRule ^(.*)_etc/cache\/([_0-9a-z]+)\-\-([_0-9a-z]+?)\-(.*?)\.s?html$ $1_etc/cgi/$2/$3.cgi?$4 [T=application/x-httpd-cgi,L]

#PROMOS
RewriteRule ^/?(.*)/pr/(.*)$ /$1 [L,R,CO=promo:$2:%{HTTP_HOST}:60:/]

