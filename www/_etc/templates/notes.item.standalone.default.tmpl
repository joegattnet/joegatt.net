<div class="grid_5 alpha">
  [% IF quote %]
    <blockquote>[% quote %]</blockquote>
  [% END %]
  [% IF attribution %]
    <p class="attribution">[% attribution %]</p>
  [% END %]
  <div class="text">
    [% text %]
  </div>
  <aside id="versions" class="meta">
    <p>Version <span class="version-source">[% version %]</span> (<abbr class="timeago" title="[% date_iso8601 %]">[% date_full %]</abbr>)</p>
  </aside>
  <div class="apparatus">
    <!--#include virtual="../../_etc/layouts/apparatus_left.shtml" -->
  </div>
</div>
<div class="grid_5 prefix_1 suffix_1 omega">
  [% FOREACH resource IN resources %]
     <a href="/_etc/resources/cut/[% resource.title %]-[% resource.rnd %]-16_9-wb-12-0-0-0.[% resource.ext %]">
      <img src="/_etc/resources/cut/[% resource.title %]-[% resource.rnd %]-16_9-wb-6-0-0-0.[% resource.ext %]" alt="[% IF caption %][% caption %][% ELSIF title %][% title %][% ELSE %][% textBriefClean %][% END %]">
     </a>
  [% END %]
  [% IF caption %]
    <p class="caption">[% caption %]</p>
  [% END %]
  [% PROCESS 'video.tmpl' vimeoWidth = 460, vimeoHeight = 350, youtubeWidth = 460, youtubeHeight = 350, dailymotionWidth = 460, dailymotionHeight = 350 %]
  [% PROCESS 'map.tmpl' %]
  <div class="apparatus">
    <div id="tags">
      <!--#include virtual="../../_etc/cache/tags--tags-tags=[% tagsUrl %].html" -->
    </div>
    <div id="notes" class="grid_5 alpha omega">
      <!--#include virtual="../../_etc/cache/notes--list-tags=[% tagsUrl %]&exclude=[% p %].html" -->
    </div>
  </div>
</div>